{
    "diffEditor.ignoreTrimWhitespace": true,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.formatOnSave": true,
    "editor.snippetSuggestions": "top",
    "editor.tabCompletion": "on",
    "emmet.triggerExpansionOnTab": true,
    "emmet.includeLanguages": {
    "*.erb": "html",
    "django-html": "html"
    },
    "explorer.compactFolders": false,
    "explorer.sortOrder": "type",
    "extensions.autoUpdate": false,
    "extensions.autoCheckUpdates": false,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 4000,
    "editor.detectIndentation": false,
    "editor.insertSpaces": true,
    "files.enableTrash": false,
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        "**/__pycache__": true,
        "**/build": true,
        "**/CVS": true,
        "**/node_modules": true
    },
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.autoStash": true,
    "git.branchSortOrder": "alphabetically",
    "gitlens.defaultDateFormat": "MMMM Do, YYYY HH:MM",
    "gitlens.defaultDateShortFormat": "MM-DD-YYYY",
    "gitlens.defaultTimeFormat": "HH:MM",
    "html.format.endWithNewline": true,
    "html.format.indentInnerHtml": true,
    "html.format.templating": true,
    "html.format.wrapLineLength": 100,
    "less.lint.float": "warning",
    "less.lint.duplicateProperties": "warning",
    "less.lint.compatibleVendorPrefixes": "warning",
    "less.lint.boxModel": "warning",
    "less.lint.idSelector": "warning",
    "less.lint.important": "warning",
    "less.lint.importStatement": "warning",
    "markdown.preview.scrollEditorWithPreview": false,
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "npm.enableRunFromFolder": true,
    "python.defaultInterpreterPath": "~/.asdf/shims/python3.10",
    "python.formatting.blackPath": "~/.local/bin/black",
    "python.formatting.provider": "black",
    "python.linting.enabled": true,
    "python.linting.flake8Args": [
        "--max-line-length=100"
    ],
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Path": "~/.local/bin/flake8",
    "python.linting.pylintEnabled": false,
    "python.poetryPath": "~/.local/bin/poetry",
    "python.testing.pytestArgs": [
        ".",
        "tests"
    ],
    "python.testing.pytestEnabled": true,
    "python.testing.pytestPath": "~/.local/bin/pytest",
    "python.testing.unittestEnabled": false,
    "python.venvFolders": [
        ".venv"
    ],
    "scss.lint.boxModel": "warning",
    "scss.lint.compatibleVendorPrefixes": "warning",
    "scss.lint.duplicateProperties": "warning",
    "scss.lint.float": "warning",
    "scss.lint.idSelector": "warning",
    "scss.lint.important": "warning",
    "scss.lint.importStatement": "warning",
    "telemetry.telemetryLevel": "off",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.env.osx": {
        "PATH": "."
    },
    "terminal.integrated.fontFamily": "Meslo LG S for Powerline",
    "update.mode": "start",
    "workbench.colorTheme": "One Dark Pro Darker",
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.settings.editor": "json",
    "workbench.settings.useSplitJSON": true,
    "workbench.startupEditor": "none",
    "zenMode.centerLayout": false,
    "zenMode.fullScreen": false,
    "zenMode.hideLineNumbers": false,
    "zenMode.hideTabs": false,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
